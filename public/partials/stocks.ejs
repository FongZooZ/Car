<div class="col-md-9">
	<uib-pagination ng-change="changePage()" total-items="totalCar" ng-model="$parent.currentPage" items-per-page="pageSize"></uib-pagination>
	<table class="table table-striped table-hover sortable-theme-bootstrap" data-sortable>
		<thead>
			<tr>
				<th ng-repeat="th in thList">{{th}}</th>
			</tr>
		</thead>
		<tbody>
			<tr ng-repeat="car in carsToDisplay">
				<td>{{($index + 1) + (currentPage - 1) * pageSize}}</td>
				<td>{{car.referenceNo}}</td>
				<td>{{car.make.name}}</td>
				<td>{{car.model.name}}</td>
				<td>${{car.fob}}</td>
				<td ng-if="!finalPrice" id="final-price">ASK</td>
				<td ng-if="finalPrice" id="final-price">${{car.pricing.finalPrice.toFixed(2)}}</td>
			</tr>
		</tbody>
	</table>
	<uib-pagination ng-change="changePage()" total-items="totalCar" ng-model="$parent.currentPage" items-per-page="pageSize"></uib-pagination>
</div>