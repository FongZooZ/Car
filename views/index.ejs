		<script type="text/javascript" src="/bower/jquery/dist/jquery.min.js"></script>
		<script type="text/javascript" src="/bower/bootstrap/dist/js/bootstrap.min.js"></script>
		<script type="text/javascript" src="/bower/angular/angular.min.js"></script>
		<script type="text/javascript" src="/bower/angular-bootstrap/ui-bootstrap.min.js"></script>
		<script type="text/javascript" src="/bower/sortable/js/sortable.min.js"></script>
		<script type="text/javascript" src="/javascripts/angular/controllers/CarController.js"></script>
